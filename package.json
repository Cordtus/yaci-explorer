{
  "name": "yaci-explorer",
  "version": "1.0.0",
  "description": "Modern block explorer for Cosmos SDK chains with EVM support",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot ./index.html",
    "build": "bun build ./index.html --outdir ./build --minify --sourcemap=linked --env PUBLIC_*",
    "start": "bunx --bun serve ./build",
    "preview": "npm run build && npm run start",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\"",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "backend:up": "docker compose -f docker/docker-compose.yml up postgres postgrest yaci",
    "panda:codegen": "panda codegen",
    "panda:watch": "panda codegen --watch",
    "clean": "rm -rf build node_modules/.vite",
    "clean:all": "rm -rf build node_modules",
    "reinstall": "npm run clean:all && npm install",
    "deploy:build": "npm run clean && npm install && npm run typecheck && npm run build"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.62.11",
    "date-fns": "^4.1.0",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "ethers": "^6.13.5",
    "isbot": "^5.1.17",
    "lucide-react": "^0.469.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.9.4"
  },
  "devDependencies": {
    "@pandacss/dev": "^1.5.1",
    "@pandacss/preset-base": "^1.5.1",
    "@park-ui/panda-preset": "^0.43.1",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@biomejs/biome": "^1.9.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "packageManager": "bun@1.3.2"
}
