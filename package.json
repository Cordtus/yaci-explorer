{
  "name": "yaci-explorer",
  "version": "1.0.0",
  "description": "Modern block explorer for Cosmos SDK chains with EVM support",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "panda codegen && vite",
    "build": "panda codegen && vite build",
    "preview": "vite preview",
    "prepare": "panda codegen",
    "panda:watch": "panda codegen --watch",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "clean": "rm -rf build dist node_modules/.vite styled-system",
    "clean:all": "rm -rf build dist node_modules styled-system",
    "reinstall": "yarn run clean:all && yarn install",
    "configure:env": "bash ./scripts/configure-env.sh",
    "deploy:build": "yarn run clean && yarn install --production=false && yarn run typecheck && yarn run build",
    "reset:full": "bash ./scripts/full-reset.sh",
    "redeploy:systemd": "bash ./scripts/redeploy-systemd.sh"
  },
  "dependencies": {
    "@ark-ui/react": "^5.29.1",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.62.11",
    "date-fns": "^4.1.0",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "isbot": "^5.1.17",
    "lucide-react": "^0.469.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.9.4"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.52.0",
    "@park-ui/panda-preset": "^0.42.0",
    "@playwright/test": "^1.56.1",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "vite": "^7.1.9"
  }
}
