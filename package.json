{
  "name": "yaci-explorer",
  "version": "1.0.0",
  "description": "Modern block explorer for Cosmos SDK chains with EVM support",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "panda codegen && vite",
    "build": "panda codegen && vite build",
    "start": "bun serve ./build",
    "preview": "bun run build && bun run start",
    "prepare": "panda codegen",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "backend:up": "docker compose -f docker/docker-compose.yml up postgres postgrest yaci",
    "panda:watch": "panda codegen --watch",
    "clean": "rm -rf build dist node_modules/.vite styled-system",
    "clean:all": "rm -rf build dist node_modules styled-system",
    "reinstall": "bun run clean:all && bun install",
    "configure:env": "bash ./scripts/configure-env.sh",
    "deploy:build": "bun install && bun run typecheck && bun run build",
    "reset:full": "bash ./scripts/full-reset.sh",
    "redeploy:systemd": "bash ./scripts/redeploy-systemd.sh"
  },
  "dependencies": {
    "@ark-ui/react": "5.29.1",
    "@tanstack/react-query": "5.90.10",
    "@tanstack/react-query-devtools": "5.91.1",
    "date-fns": "4.1.0",
    "echarts": "5.6.0",
    "echarts-for-react": "3.0.5",
    "isbot": "5.1.32",
    "lucide-react": "0.554.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@pandacss/dev": "1.5.1",
    "@pandacss/preset-base": "1.5.1",
    "@park-ui/panda-preset": "0.43.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "postcss": "8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-load-config": "^6.0.1",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "vite": "^7.2.4"
  },
  "packageManager": "bun@1.3.2"
}
